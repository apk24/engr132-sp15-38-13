function [ mats ] = generateRandomMats( num )


symbols = ['a':'z' 'A':'Z' '0':'9'];
 MAX_ST_LENGTH = 50;
for ct = num:-1:1
    
 stLength = randi(MAX_ST_LENGTH);
 nums = randi(numel(symbols),[1 stLength]);
 st = symbols (nums);
    mats(ct) = struct(...
    'qdeg', 10*rand()+5, ...
    'beg', 10*rand()+5, ...
    'r', 9*rand()+1, ...
    'eps', rand(), ...
    'cost', rand()*100, ...
    'tox', rand()*6, ...
    'name', 'a' ...
    );

end

